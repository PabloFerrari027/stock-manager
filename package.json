{
  "name": "stock-manager-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prisma:migrate": "npx prisma migrate dev --schema=./src/shared/infra/prisma/postgreSQL/schema.prisma",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema=./src/shared/infra/prisma/postgreSQL/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/shared/infra/prisma/postgreSQL/schema.prisma",
    "prisma:seed": "tsx src/shared/infra/prisma/postgreSQL/seed.ts",
    "prisma:studio": "npx prisma studio --schema=./src/shared/infra/prisma/postgreSQL/schema.prisma",
    "docker:compose": "docker-compose --project-name stock_manager_dev -f docker/docker-compose.dev.yml up -d && docker-compose --project-name stock_manager_prod -f docker/docker-compose.prod.yml up -d",
    "dev": "tsx watch src/shared/infra/http/server.ts",
    "build": "tsc -p ./tsconfig.build.json && tsc-alias",
    "start": "node -r tsconfig-paths/register build/shared/infra/http/server.js",
    "prettier:format": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "plop": "plop",
    "test:unit": "vitest -c ./vite.config.unit.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "tsc-alias": "^1.8.8",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
